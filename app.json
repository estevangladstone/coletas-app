{
  "expo": {
    "name": "Coletas-App",
    "slug": "Coletas-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imgs/app_icon.png",
    "splash": {
      "image": "./src/assets/imgs/app_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/imgs/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.estevangnm.Project",
      "permissions": [ "CAMERA", "ACCESS_FINE_LOCATION", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE" ]
    },
    "web": {
      "favicon": "./src/assets/imgs/app_icon.png"
    }
  }
}
