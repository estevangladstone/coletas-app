{
  "expo": {
    "name": "Project",
    "slug": "Project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imgs/notebook_icon.png",
    "splash": {
      "image": "./src/assets/imgs/notebook_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/imgs/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.estevangnm.Project",
      "permissions": [ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL" , "LOCATION" ]
    },
    "web": {
      "favicon": "./src/assets/imgs/favicon.png"
    }
  }
}
